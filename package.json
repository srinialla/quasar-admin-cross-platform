{
  "name": "quasar-admin-cross-platform-apps",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "srinireddy.alla@gmail.com",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:pwa": "quasar dev -m pwa",
    "dev:desktop": " quasar dev -m electron",
    "dev:android": "quasar dev -m capacitor -T android",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "build": "npm cache clean --force && npm i && npx quasar build",
    "prod:pwa": "quasar build -m pwa",
    "prod:desktop": "quasar build -m electron",
    "prod:android": "quasar build -m capacitor -T android",
    "prod:ios": "quasar build -m capacitor -T ios"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@quasar/quasar-ui-qcalendar": "^4.0.0-beta.16",
    "axios": "^1.2.1",
    "echarts": "^5.4.1",
    "events": "^3.3.0",
    "pinia": "^2.0.11",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-echarts": "^6.5.4",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.12",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.3",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.9.0",
    "@quasar/vite-plugin": "^1.7.0",
    "autoprefixer": "^10.4.2",
    "electron": "^31.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "quasar-app-extension-qhierarchy": "^1.0.9",
    "vite-plugin-checker": "^0.7.1",
    "workbox-build": "^7.1.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
